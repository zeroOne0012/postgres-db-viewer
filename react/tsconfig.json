  "compilerOptions": {
    "jsx": "react",
    "target": "es2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["es2017", "dom"],
    "baseUrl": ".",         
    "paths": {
      "*": ["src/*"]                
    },
  },
  "include": ["src"],
}           
